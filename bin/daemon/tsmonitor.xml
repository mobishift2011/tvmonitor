<service>
	<id>tsmonitor.exe</id>
	<name>tsmonitor</name>
	<description>The nodejs.org example web server.</description>
	<executable>D:\Program Files\nodejs\node.exe</executable>
	<logmode>rotate</logmode>
	<argument>--harmony</argument>
	<argument>C:\Users\lisheng\AppData\Roaming\npm\node_modules\node-windows\lib\wrapper.js</argument>
	<argument>--file</argument>
	<argument>D:\ts-monitor\bin\www</argument>
	<argument>--log</argument>
	<argument>tsmonitor wrapper</argument>
	<argument>--grow</argument>
	<argument>0.25</argument>
	<argument>--wait</argument>
	<argument>1</argument>
	<argument>--maxrestarts</argument>
	<argument>3</argument>
	<argument>--abortonerror</argument>
	<argument>n</argument>
	<argument>--stopparentfirst</argument>
	<argument>undefined</argument>
	<stoptimeout>30sec</stoptimeout>
	<workingdirectory>D:\ts-monitor</workingdirectory>
</service>